{
	"name": "@socialyze/api",
	"version": "1.0.0",
	"scripts": {
		"dev": "node --import tsx --watch src/index.ts",
		"build": "tsc",
		"start": "node dist/index.js",
		"test": "vitest run --config vitest.config.ts",
		"lint": "eslint 'src/**' 'tests/**' --max-warnings=0",
		"seed:fake-users": "pnpm exec ts-node src/scripts/fakeUsers.ts",
		"seed:fake-posts-with-comments": "pnpm exec ts-node src/scripts/fakePostsWithComments.ts"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.817.0",
		"@aws-sdk/s3-request-presigner": "^3.817.0",
		"@faker-js/faker": "^9.8.0",
		"@socialyze/shared": "workspace:*",
		"cors": "2.8.5",
		"dotenv": "16.4.1",
		"express": "5.1.0",
		"jsonwebtoken": "9.0.2",
		"mongodb": "^6.16.0",
		"mongoose": "8.15.0",
		"pino": "^9.7.0",
		"pino-pretty": "^13.0.0",
		"supertest": "^7.1.1",
		"vitest": "^3.1.4",
		"zod": "3.22.4"
	},
	"devDependencies": {
		"@types/cors": "2.8.17",
		"@types/express": "4.17.21",
		"@types/jsonwebtoken": "9.0.5",
		"@types/node": "20.11.19",
		"@types/pino": "^7.0.5",
		"@types/supertest": "^6.0.3",
		"mongodb-memory-server": "^10.1.4",
		"ts-node": "^10.9.2",
		"tsx": "4.19.4"
	}
}
